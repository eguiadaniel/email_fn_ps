<mjml>
  <mj-head>
    <mj-title>Core</mj-title>
    <mj-preview>Core Preview</mj-preview>
    <mj-breakpoint width="320px" />
    <mj-attributes>
      <mj-text padding="0" />
      <mj-class name="main-text" color="#000" font-size="35px" font-family="arial" font-weight="bold" margin="0px" padding="0px" text-align="center" line-height="120%" />
      <mj-class name="footer-icon-text" color="#fff" font-size="15px" letter-spacing="2" font-family="arial" font-weight="bold" text-align="center" line-height="150%" />
    </mj-attributes>
    <mj-style inline="inline">
      .center-text div { text-align: center !important; } .footer-icon-text p{ margin: 10px 0px; padding: 0px; }
    </mj-style>
  </mj-head>

  <mj-body background-color="#fff">



    <!-- Start of core content-->

    <!-- This is Header Section-->
    <mj-section padding="20px 0px 20px 0px">
      <mj-column>
        <mj-image width="232px" src="https://fantasynails.es/imagenes/email/header/logo-header.png"></mj-image>
      </mj-column>
    </mj-section>

    <!-- This is Main Section-->
    <mj-wrapper border="1px solid #000000" padding="0px 0px">
      <mj-section padding="0px 0px 20px 0px">
        <mj-column>

          <mj-divider border-color="#000" border-width="1px" padding="0px"></mj-divider>

          <mj-text mj-class="main-text" css-class="center-text">Â¡Bienvenida {firstname}!</mj-text>
        </mj-column>
      </mj-section>
    </mj-wrapper>

    <!-- This is Footer Section-->
    <mj-wrapper border="0px solid #000000" padding="0px 0px 0px 0px" background-color="#000">
      <mj-section padding="0px 0px" margin="0px">
                  <mj-column width="80px">
            <mj-image src="https://fantasynails.es/imagenes/email/footer/footer-icon-bag.png" alt="" align="right" border="none" width="72px" padding-left="0px" padding-right="10px" padding-bottom="10px" padding-top="10px"></mj-image>
          </mj-column>
          <mj-column width="200px">
            <mj-text mj-class="footer-icon-text" css-class="footer-icon-text">
              <p>Visita nuestra tienda <br> <span style="font-weight: lighter;">Compra online</span></p>
            </mj-text>
          </mj-column>
      </mj-section>
   
      <mj-section padding="0px 0px" margin="0px">
                  <mj-column width="80px">
            <mj-image src="https://fantasynails.es/imagenes/email/footer/footer-icon-nail.png" alt="" align="right" border="none" width="72px" padding-left="0px" padding-right="10px" padding-bottom="10px" padding-top="10px"></mj-image>
          </mj-column>
          <mj-column width="200px">
            <mj-text mj-class="footer-icon-text" css-class="footer-icon-text">
              <p>Descubre tendencias <br> <span style="font-weight: lighter;">Visita nuestros looks</span></p>
            </mj-text>
          </mj-column>
      </mj-section>
        <mj-section padding="0px">
      <mj-column>
        <mj-social font-size="15px" icon-size="40px" mode="horizontal">
           <mj-social-element href="url" src="https://fantasynails.es/imagenes/email/footer/fantasy.png" background-color="#000" href="https://mjml.io/" icon-padding="7px"></mj-social-element>
          <mj-social-element name="facebook-noshare" background-color="#000" href="https://mjml.io/" icon-padding="7px">
            
          </mj-social-element>
          <mj-social-element name="instagram-noshare" background-color="#000" href="https://mjml.io/" icon-padding="7px">
            
          </mj-social-element>
          <mj-social-element  name="youtube-noshare" background-color="#000" href="https://mjml.io/" icon-padding="7px">
            
          </mj-social-element>
         
        </mj-social>
      </mj-column>
    </mj-section>
    </mj-wrapper>

    <!-- End of core content-->
  </mj-body>
</mjml>